<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @project.title %>
</p>

<table class="users">
	<tr>
		<th>Project contributors</th>
	</tr>
	<% for user in @project.users %>
	<tr>
		<td><%= user.name %></td>
	</tr>
	<% end %>
</table>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
